<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Pfa\ProjectBundle\Entity\UserCredentials" table="user_credentials">
    <indexes>
      <index name="fk1_u" columns="employee_id"/>
    </indexes>
    <unique-constraints>
      <unique-constraint name="uk1_u" columns="username"/>
    </unique-constraints>
    <id name="userId" type="integer" column="user_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="username" type="string" column="username" length="255" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="default">NULL</option>
      </options>
    </field>
    <field name="password" type="string" column="password" length="255" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="default">NULL</option>
      </options>
    </field>
    <field name="enabled" type="boolean" column="enabled" nullable="true">
      <options>
        <option name="default">1</option>
      </options>
    </field>
    <field name="role" type="string" column="role" length="255" nullable="true">
      <options>
        <option name="fixed"/>
        <option name="default">NULL</option>
      </options>
    </field>
    <many-to-one field="employee" target-entity="Employees" fetch="LAZY">
      <join-columns>
        <join-column name="employee_id" referenced-column-name="employee_id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
